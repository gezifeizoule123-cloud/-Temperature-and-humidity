}

/* 纯文本编辑框 - 网易云黑 */
QPlainTextEdit {
    background-color: #2d2d2d;  /* 网易云黑 */
    border: 1px solid #404040;
    border-radius: 4px;
    padding: 6px 8px;
    color: #e0e0e0;  /* 浅灰色文字 */
    font-size: 14px;
    selection-background-color: #1db954;
}

/* 表格视图 - 网易云黑 */
QTableView {
    background-color: #2d2d2d;  /* 网易云黑 */
    border: 1px solid #404040;
    border-radius: 4px;
    color: #e0e0e0;
    gridline-color: #404040;
    alternate-background-color: #333333;
}

QTableView::item {
    background-color: #2d2d2d;
    color: #e0e0e0;
    border: none;
    padding: 4px;
}

QTableView::item:selected {
    background-color: #1db954;
    color: #ffffff;
}

QTableView::item:hover {
    background-color: #3a3a3a;
}

QHeaderView::section {
    background-color: #404040;
    color: #e0e0e0;
    padding: 6px;
    border: none;
    font-weight: bold;
}

/* 复选框样式 */
QCheckBox {
    background-color: transparent;
    color: #5c3713;
    spacing: 8px;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #b08f6d;
    border-radius: 3px;
    background-color: #ffffff;
}

QCheckBox::indicator:checked {
    background-color: #d2691e;
    border: 1px solid #d2691e;
}

/* 滑动条样式 */
QSlider::groove:horizontal {
    border: 1px solid #b08f6d;
    height: 4px;
    background-color: #e8d5c4;
    border-radius: 2px;
}

QSlider::handle:horizontal {
    background-color: #d2691e;
    border: 1px solid #d2691e;
    width: 16px;
    height: 16px;
    border-radius: 8px;
    margin: -6px 0;
}

QSlider::handle:horizontal:hover {
    background-color: #e37c2b;
    border: 1px solid #e37c2b;
}

/* 按钮样式 - 保持原有粉色效果 */
QPushButton,
QToolButton {
    border: none;
    background-color: transparent;
    outline: none;
    padding: 8px 12px;
    border-radius: 4px;
    font-size: 14px;
    color: #5c3713;  /* 棕色文字 */
}

QPushButton:hover,
QToolButton:hover {
    background-color: #FFE4E6;  /* 淡粉色 */
    border: none;
    color: #333333;
}

QPushButton:pressed,
QToolButton:pressed {
    background-color: #F3E8FF;  /* 淡紫色 */
    border: none;
    color: #333333;
}

QPushButton:focus,
QToolButton:focus {
    outline: none;
    border: none;
}

/* 特殊按钮样式 */
QPushButton#clear_yu_bt,
