  <property name="styleSheet">
   <string notr="true">/* 网易云音乐风格暗色主题 */
QMainWindow {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                                stop:0 #1a1a1a, stop:0.5 #2d2d2d, stop:1 #1e1e1e);
    border: none;
}

/* 分组框样式 - 网易云风格的卡片式设计 */
QMainWindow QGroupBox {
    background-color: rgba(40, 40, 40, 0.8);
    border: 1px solid #404040;
    border-radius: 8px;
    margin-top: 10px;
    padding-top: 10px;
    color: #e0e0e0;
    font-weight: bold;
}

QMainWindow QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 8px;
    background-color: #2d2d2d;
    color: #ffffff;
    border-radius: 4px;
}

/* Frame样式 */
QMainWindow QFrame {
    background-color: rgba(45, 45, 45, 0.9);
    border: 1px solid #404040;
    border-radius: 6px;
}

/* 标签样式 */
QMainWindow QLabel {
    background-color: transparent;
    color: #e0e0e0;
    font-size: 14px;
}

/* 特殊标签 - 时间显示等 */
QMainWindow QLabel#time_1,
QMainWindow QLabel[label_4] {
    color: #ffffff;
    font-size: 16px;
    font-weight: bold;
}

/* 状态标签 - WiFi、开关状态 */
QMainWindow QLabel#labelDUANKAI,
QMainWindow QLabel#label_6wIFI {
    color: #1db954; /* 网易云绿 */
    font-weight: bold;
}

/* 输入框样式 */
QMainWindow QLineEdit {
    background-color: #333333;
    border: 1px solid #555555;
    border-radius: 4px;
    padding: 6px 8px;
    color: #ffffff;
    font-size: 14px;
    selection-background-color: #1db954;
}

QMainWindow QLineEdit:focus {
    border: 1px solid #1db954;
    background-color: #3a3a3a;
}

/* 复选框样式 */
QMainWindow QCheckBox {
    background-color: transparent;
    color: #e0e0e0;
    spacing: 8px;
}

QMainWindow QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #666666;
    border-radius: 3px;
    background-color: #333333;
}

QMainWindow QCheckBox::indicator:checked {
    background-color: #1db954;
    border: 1px solid #1db954;
    image: url(:/checked.png); /* 可以设置对勾图标 */
}

QMainWindow QCheckBox::indicator:hover {
    border: 1px solid #888888;
}

/* 滑动条样式 */
QMainWindow QSlider::groove:horizontal {
    border: 1px solid #444444;
    height: 4px;
    background-color: #444444;
    border-radius: 2px;
}

QMainWindow QSlider::handle:horizontal {
    background-color: #1db954;
    border: 1px solid #1db954;
    width: 16px;
    height: 16px;
    border-radius: 8px;
    margin: -6px 0;
}

QMainWindow QSlider::handle:horizontal:hover {
    background-color: #1ed760;
    border: 1px solid #1ed760;
}

/* 按钮样式保持不变 */
QMainWindow QPushButton,
QMainWindow QToolButton {
    border: none;
    background-color: transparent;
    outline: none;
    padding: 8px 12px;
    border-radius: 4px;
    font-size: 14px;
    color: #e0e0e0;
}

QMainWindow QPushButton:hover,
QMainWindow QToolButton:hover {
    background-color: #FFE4E6;
    border: none;
    color: #333333;
}

QMainWindow QPushButton:pressed,
QMainWindow QToolButton:pressed {
    background-color: #F3E8FF;
    border: none;
    color: #333333;
}

QMainWindow QPushButton:focus,
QMainWindow QToolButton:focus {
    outline: none;
    border: none;
}

/* 特殊按钮 - 清空、设置按钮 */
QMainWindow QPushButton#clear_yu_bt,
QMainWindow QPushButton#set_yu_bt,
QMainWindow QPushButton#set_light_bt {
    background-color: #404040;
    color: #ffffff;
}

QMainWindow QPushButton#clear_yu_bt:hover,
QMainWindow QPushButton#set_yu_bt:hover,
QMainWindow QPushButton#set_light_bt:hover {
    background-color: #4a4a4a;
}

/* 线条分隔符 */
QMainWindow Line {
    background-color: #404040;
    border: none;
}</string>
